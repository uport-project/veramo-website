{
  "name": "veramo-site-docs",
  "version": "4.1.2",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production HTTPS=true docusaurus serve --port $PORT",
    "dev": "docusaurus start",
    "serve": "docusaurus serve",
    "docusaurus": "docusaurus",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "build:veramo": "bash ./scripts/build-veramo.sh",
    "format": "prettier --write '{src,docs, !build}/**/*'"
  },
  "dependencies": {
    "@algolia/client-search": "^4.14.3",
    "@docusaurus/core": "2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "did-resolver": "^4.0.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@docusaurus/module-type-aliases": "^2.2.0",
    "@docusaurus/types": "^2.2.0",
    "@microsoft/api-documenter": "^7.20.1",
    "@microsoft/api-extractor": "^7.34.0",
    "@tsconfig/docusaurus": "^1.0.6",
    "@types/node": "^18.11.18",
    "@types/react": "^16.9.56",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "fs-extra": "^11.1.0",
    "husky": "^4.3.0",
    "path": "^0.12.7",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "react-github-btn": "^1.4.0",
    "readline": "^1.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">16"
  }
}
